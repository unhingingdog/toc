<h1> Disputes </h1>
<%= link_to "New Dispute", new_dispute_path %>

<ul>
  <% @disputes.each do |dispute| %>
    <% if dispute.activated? %>
      <li><%= link_to dispute.title, dispute %></li>
    <% else %>
    <li><%= dispute.title + " (Not yet accepted by respondent)" %>
    <% end %>
  <% end %>
</ul>
